    <field name="reason" type="string" column="reason" length="256" nullable="false"/>
    <field name="waring" type="string" column="waring" length="256" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
    <field name="deviceId" type="string" column="device_id" length="40" nullable="false"/>

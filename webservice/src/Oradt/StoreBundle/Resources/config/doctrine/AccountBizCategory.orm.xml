<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\AccountBizCategory" table="account_biz_category">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bizId" type="string" column="biz_id" length="40" nullable="false"/>
    <field name="categoryId" type="string" column="category_id" length="10" nullable="false"/>
    <field name="createTime" type="integer" column="create_time" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>

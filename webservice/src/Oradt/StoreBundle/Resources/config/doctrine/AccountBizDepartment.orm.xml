<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\AccountBizDepartment" table="account_biz_department">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="departId" type="string" column="depart_id" length="40" nullable="false"/>
    <field name="name" type="string" column="name" length="90" nullable="false"/>
    <field name="ename" type="string" column="ename" length="90" nullable="true"/>
    <field name="type" type="integer" column="type" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
    <field name="bizId" type="string" column="biz_id" length="40" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="language" type="integer" column="language" nullable="false"/>
  </entity>
</doctrine-mapping>

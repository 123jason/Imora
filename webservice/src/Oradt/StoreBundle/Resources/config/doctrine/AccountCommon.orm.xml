<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\AccountCommon" table="account_common">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" nullable="true"/>
    <field name="accountId" type="string" column="account_id" length="40" nullable="false"/>
    <field name="account" type="string" column="account" length="50" nullable="false"/>
    <field name="createtime" type="integer" column="createtime" nullable="false"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\AccountCommunicate" table="account_communicate">
    <indexes>
      <index name="mobile" columns="mobile"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="name" type="string" column="name" length="96" nullable="false"/>
    <field name="mobile" type="string" column="mobile" length="18" nullable="false"/>
    <field name="mcode" type="string" column="mcode" length="4" nullable="false"/>
    <field name="status" type="string" column="status" nullable="false"/>
    <field name="creatTime" type="integer" column="creat_time" nullable="false"/>
    <field name="issend" type="integer" column="issend" nullable="false"/>
    <field name="sendTime" type="integer" column="send_time" nullable="false"/>
    <field name="friendTime" type="integer" column="friend_time" nullable="false"/>
    <field name="fuserId" type="string" column="fuser_id" length="40" nullable="false"/>
    <field name="popularType" type="integer" column="popular_type" nullable="false"/>
    <field name="cardId" type="string" column="card_id" length="40" nullable="true"/>
    <field name="popularTime" type="integer" column="popular_time" nullable="false"/>
  </entity>
</doctrine-mapping>

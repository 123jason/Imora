<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\BizOperatorExtendInfo" table="biz_operator_extend_info">
    <unique-constraints>
      <unique-constraint name="biz_id_UNIQUE" columns="biz_id"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bizId" type="string" column="biz_id" length="40" nullable="false"/>
    <field name="idcardCopyPath" type="string" column="idcard_copy_path" length="256" nullable="false"/>
    <field name="authorityCopyPath" type="string" column="authority_copy_path" length="256" nullable="false"/>
    <field name="licenseCopyPath" type="string" column="license_copy_path" length="256" nullable="false"/>
    <field name="codeCopyPath" type="string" column="code_copy_path" length="256" nullable="false"/>
    <field name="legalidcardCopyPath" type="string" column="legalidcard_copy_path" length="256" nullable="false"/>
    <field name="updateTime" type="datetime" column="update_time" nullable="false"/>
    <field name="adminId" type="string" column="admin_id" length="40" nullable="false"/>
    <field name="status" type="string" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>

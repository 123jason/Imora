    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="cardNum" type="integer" column="card_num" nullable="false"/>
    <field name="accountNum" type="integer" column="account_num" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
    <field name="modifyTime" type="integer" column="modify_time" nullable="false"/>
    <field name="adminId" type="string" column="admin_id" length="40" nullable="false"/>
    <field name="cancelAdminId" type="string" column="cancel_admin_id" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>

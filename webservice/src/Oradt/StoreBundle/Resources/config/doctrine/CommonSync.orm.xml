<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\CommonSync" table="common_sync">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="string" column="user_id" length="40" nullable="true"/>
    <field name="modifedtime" type="datetime" column="modifedtime" nullable="true"/>
    <field name="moduleid" type="string" column="moduleid" length="40" nullable="true"/>
    <field name="module" type="string" column="module" nullable="true"/>
    <field name="operation" type="string" column="operation" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\ContactCardExchangeGroup" table="contact_card_exchange_group">
    <indexes>
      <index name="created_time" columns="created_time"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="groupId" type="string" column="group_id" length="6" nullable="false"/>
    <field name="groupName" type="string" column="group_name" length="30" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

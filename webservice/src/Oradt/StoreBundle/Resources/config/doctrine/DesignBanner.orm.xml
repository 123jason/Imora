<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignBanner" table="design_banner">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="45" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="true"/>
    <field name="linkUrl" type="string" column="link_url" length="255" nullable="true"/>
    <field name="sort" type="integer" column="sort" nullable="true"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="true"/>
    <field name="bannerId" type="string" column="banner_id" length="32" nullable="false"/>
  </entity>
</doctrine-mapping>

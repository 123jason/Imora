      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="string" column="user_id" length="40" nullable="true"/>
    <field name="number" type="decimal" column="number" precision="13" scale="2" nullable="true"/>
    <field name="unitId" type="integer" column="unit_id" nullable="true"/>

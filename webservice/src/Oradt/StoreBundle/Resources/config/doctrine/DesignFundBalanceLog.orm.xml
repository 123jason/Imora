    <field name="number1" type="decimal" column="number1" precision="13" scale="2" nullable="true"/>
    <field name="unitId" type="integer" column="unit_id" nullable="true"/>
    <field name="insertTime" type="integer" column="insert_time" nullable="true"/>
    <field name="type" type="string" column="type" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="itemId" type="string" column="item_id" length="32" nullable="true"/>
    <field name="itemName" type="string" column="item_name" length="255" nullable="true"/>
    <field name="account" type="string" column="account" length="100" nullable="true"/>

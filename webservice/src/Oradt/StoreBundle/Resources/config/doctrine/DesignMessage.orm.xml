<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignMessage" table="design_message">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="messageId" type="string" column="message_id" length="32" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="true"/>
    <field name="content" type="text" column="content" nullable="true"/>
    <field name="userid" type="string" column="userid" length="40" nullable="true"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="status" type="boolean" column="status" nullable="true"/>
    <field name="type" type="boolean" column="type" nullable="false"/>
  </entity>
</doctrine-mapping>

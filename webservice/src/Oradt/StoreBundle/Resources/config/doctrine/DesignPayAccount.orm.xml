<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignPayAccount" table="design_pay_account">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accountId" type="string" column="account_id" length="32" nullable="false"/>
    <field name="userid" type="string" column="userid" length="40" nullable="false"/>
    <field name="type" type="integer" column="type" nullable="true"/>
    <field name="payName" type="string" column="pay_name" length="255" nullable="true"/>
    <field name="payAccount" type="string" column="pay_account" length="255" nullable="true"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="updateTime" type="integer" column="update_time" nullable="true"/>
  </entity>
</doctrine-mapping>

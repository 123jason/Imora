<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignProductDetail" table="design_product_detail">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="productId" type="string" column="product_id" length="32" nullable="false"/>
    <field name="name" type="string" column="name" length="96" nullable="false"/>
    <field name="type" type="integer" column="type" nullable="false"/>
    <field name="price" type="decimal" column="price" precision="13" scale="2" nullable="false"/>
    <field name="unitId" type="integer" column="unit_id" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="author" type="string" column="author" length="96" nullable="false"/>
    <field name="size" type="integer" column="size" nullable="false"/>
    <field name="url" type="string" column="url" length="256" nullable="false"/>
    <field name="binUrl" type="string" column="bin_url" length="256" nullable="false"/>
    <field name="styleId" type="string" column="style_id" length="32" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
    <field name="sales" type="bigint" column="sales" nullable="false"/>
    <field name="explains" type="string" column="explains" length="10240" nullable="true"/>
    <field name="designFile" type="string" column="design_file" length="256" nullable="true"/>
    <field name="isAuth" type="integer" column="is_auth" nullable="false"/>
    <field name="checkUserid" type="string" column="check_userid" length="40" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="updateTime" type="integer" column="update_time" nullable="false"/>
  </entity>
</doctrine-mapping>

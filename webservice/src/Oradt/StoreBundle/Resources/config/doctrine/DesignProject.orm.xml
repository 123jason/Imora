<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignProject" table="design_project">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="projectId" type="string" column="project_id" length="32" nullable="true"/>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="title" type="string" column="title" length="45" nullable="true"/>
    <field name="content" type="string" column="content" length="255" nullable="true"/>
    <field name="contact" type="string" column="contact" length="45" nullable="true"/>
    <field name="styleId" type="string" column="style_id" length="32" nullable="true"/>
    <field name="color" type="string" column="color" length="45" nullable="true"/>
    <field name="tplContent" type="string" column="tpl_content" length="45" nullable="true"/>
    <field name="location" type="string" column="location" length="45" nullable="true"/>
    <field name="deadlineTime" type="integer" column="deadline_time" nullable="true"/>
    <field name="price" type="decimal" column="price" precision="8" scale="2" nullable="true"/>
	<field name="unitId" type="integer" column="unit_id" precision="8" scale="2" nullable="true"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="updateTime" type="integer" column="update_time" nullable="true"/>
    <field name="type" type="integer" column="type" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="true"/>
    <field name="endTime" type="integer" column="end_time" nullable="true"/>
  </entity>
</doctrine-mapping>

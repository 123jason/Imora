<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\DesignWorks" table="design_works">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="name" type="string" column="name" length="45" nullable="true"/>
    <field name="worksId" type="string" column="works_id" length="32" nullable="false"/>
    <field name="author" type="string" column="author" length="45" nullable="true"/>
    <field name="explains" type="string" column="explains" length="10240" nullable="true"/>
    <field name="typeId" type="integer" column="type_id" nullable="true"/>
    <field name="authorize" type="integer" column="authorize" nullable="true"/>
	<field name="hits" type="integer" column="hits" nullable="true"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="updateTime" type="integer" column="update_time" nullable="true"/>
	<field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>

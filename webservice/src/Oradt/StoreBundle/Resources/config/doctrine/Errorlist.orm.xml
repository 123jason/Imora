<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\Errorlist" table="errorlist">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="source" type="string" column="source" length="255" nullable="false"/>
    <field name="module" type="string" column="module" length="255" nullable="false"/>
    <field name="content" type="text" column="content" nullable="false"/>
    <field name="version" type="string" column="version" length="50" nullable="false"/>
    <field name="insertTime" type="datetime" column="insert_time" nullable="false"/>
  </entity>
</doctrine-mapping>

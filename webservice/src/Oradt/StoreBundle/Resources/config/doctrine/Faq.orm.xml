<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\Faq" table="faq">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="questionid" type="string" column="questionid" length="32" nullable="false"/>
    <field name="languageid" type="integer" column="languageid" nullable="false"/>
    <field name="question" type="string" column="question" length="255" nullable="false"/>
    <field name="answer" type="text" column="answer" nullable="false"/>
    <field name="sort" type="integer" column="sort" nullable="false"/>
    <field name="zan" type="integer" column="zan" nullable="true"/>
    <field name="cai" type="integer" column="cai" nullable="true"/>
    <field name="typeid" type="integer" column="typeid" nullable="false"/>
  </entity>
</doctrine-mapping>

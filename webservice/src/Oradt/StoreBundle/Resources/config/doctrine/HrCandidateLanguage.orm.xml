<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\HrCandidateLanguage" table="hr_candidate_language">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="candidateId" type="string" column="candidate_id" length="32" nullable="false"/>
    <field name="languageId" type="string" column="language_id" length="32" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="40" nullable="true"/>
    <field name="name" type="string" column="name" length="30" nullable="false"/>
    <field name="level" type="integer" column="level" nullable="false"/>
    <field name="state" type="string" column="state" nullable="false"/>
    <field name="createTime" type="integer" column="create_time" nullable="true"/>
    <field name="levelValue" type="integer" column="level_value" nullable="true"/> 
  </entity>
</doctrine-mapping>

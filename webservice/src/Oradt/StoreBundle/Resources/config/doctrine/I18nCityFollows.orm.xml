<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\I18nCityFollows" table="i18n_city_follows">
    <indexes>
      <index name="user_id" columns="user_id"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uuid" type="string" column="uuid" length="32" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="40" nullable="false"/>
    <field name="citycode" type="string" column="citycode" length="16" nullable="false"/>
    <field name="createdTime" type="datetime" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

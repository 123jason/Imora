<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OradtTask" table="oradt_task">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="integer" column="type" nullable="false"/>
    <field name="redeemCodeGroupid" type="integer" column="redeem_code_groupid" nullable="false"/>
    <field name="uptime" type="integer" column="uptime" nullable="false"/>
    <field name="downtime" type="integer" column="downtime" nullable="false"/>
    <field name="content" type="string" column="content" length="500" nullable="false"/>
    <field name="adminId" type="string" column="admin_id" length="40" nullable="false"/>
    <field name="releaseAdminId" type="string" column="release_admin_id" length="40" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OradtTaskUser" table="oradt_task_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="taskId" type="integer" column="task_id" nullable="false"/>
    <field name="accountId" type="string" column="account_id" length="40" nullable="false"/>
    <field name="count" type="integer" column="count" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

    </id>
    <field name="companyName" type="string" column="company_name" length="200" nullable="false"/>
    <field name="companyAlias" type="string" column="company_alias" length="250" nullable="false"/>
    <field name="keywordsCn" type="string" column="keywords_cn" length="200" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
    <field name="modifyTime" type="integer" column="modify_time" nullable="false"/>
    <field name="commitTime" type="integer" column="commit_time" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OrangeExtractRule" table="orange_extract_rule">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ruleType" type="string" column="rule_type" nullable="true"/>
    <field name="contentType" type="integer" column="content_type" nullable="false"/>
    <field name="cardType" type="integer" column="card_type" nullable="false"/>
    <field name="pushUnits" type="integer" column="push_units" nullable="false"/>
    <field name="content" type="text" column="content" nullable="false"/>
    <field name="source" type="string" column="source" length="100" nullable="false"/>
    <field name="keyword" type="string" column="keyword" length="250" nullable="false"/>
    <field name="sign" type="string" column="sign" length="100" nullable="false"/>
    <field name="extractInfo" type="string" column="extract_info" length="255" nullable="false"/>
    <field name="modifyTime" type="integer" column="modify_time" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
  </entity>
</doctrine-mapping>

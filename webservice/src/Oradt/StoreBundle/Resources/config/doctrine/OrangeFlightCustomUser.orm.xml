<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OrangeFlightCustomUser" table="orange_flight_custom_user">
    <indexes>
      <index name="a_id_x" columns="account_id"/>
      <index name="f_id_x" columns="fid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fid" type="integer" column="fid" nullable="false"/>
    <field name="accountId" type="string" column="account_id" length="40" nullable="false"/>
    <field name="modifyTime" type="integer" column="modify_time" nullable="false"/>
    <field name="createdTime" type="integer" column="created_time" nullable="false"/>
    <field name="sendstate" type="string" column="sendstate" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>

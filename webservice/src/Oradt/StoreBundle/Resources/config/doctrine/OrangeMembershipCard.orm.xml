<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OrangeMembershipCard" table="orange_membership_card">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cardType" type="integer" column="card_type" nullable="true"/>
    <field name="cardTypeTwo" type="integer" column="card_type_two" nullable="true"/>
    <field name="cardUnits" type="integer" column="card_units" nullable="true"/>
    <field name="description" type="string" column="description" length="250" nullable="true"/>
    <field name="keyword" type="string" column="keyword" length="180" nullable="true"/>
    <field name="agreement" type="text" column="agreement" nullable="true"/>
    <field name="rule" type="string" column="rule" length="500" nullable="true"/>
    <field name="picPathA" type="string" column="pic_path_a" length="200" nullable="false"/>
    <field name="picPathB" type="string" column="pic_path_b" length="200" nullable="false"/>
    <field name="snapshot" type="string" column="snapshot" length="200" nullable="false"/>
    <field name="vcard" type="text" column="vcard" nullable="false"/>
    <field name="persondata" type="text" column="persondata" nullable="true"/>
    <field name="searchedkwd" type="text" column="searchedkwd" nullable="true"/>
    <field name="searchingkwd" type="text" column="searchingkwd" nullable="true"/>
    <field name="isSynch" type="boolean" column="is_synch" nullable="false"/>
    <field name="tempNumber" type="string" column="temp_number" length="15" nullable="true"/>
    <field name="cardTypeUnitsNumber" type="integer" column="card_type_units_number" nullable="true"/>
    <field name="isShow" type="boolean" column="is_show" nullable="true"/>
    <field name="tagModifyTime" type="integer" column="tag_modify_time" nullable="true"/>
    <field name="modifyTime" type="integer" column="modify_time" nullable="true"/>
    <field name="createdTime" type="integer" column="created_time" nullable="true"/>
  </entity>
</doctrine-mapping>

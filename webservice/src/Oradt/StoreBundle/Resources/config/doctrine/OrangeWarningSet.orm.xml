<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\OrangeWarningSet" table="orange_warning_set">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="40" nullable="false"/>
    <field name="email" type="string" column="email" length="80" nullable="false"/>
    <field name="mobile" type="string" column="mobile" length="18" nullable="false"/>
    <field name="createtime" type="integer" column="createtime" nullable="false"/>
  </entity>
</doctrine-mapping>

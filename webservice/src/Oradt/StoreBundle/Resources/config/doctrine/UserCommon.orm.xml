<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\UserCommon" table="user_common">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="userName" type="string" column="username" length="255"/>
    <field name="accountNo" type="string" column="account_no" length="255"/>
    <field name="mobile" type="string" column="mobile" length="255"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="password" type="string" column="password" length="255"/>
    <field name="bizId" type="string" column="biz_id" length="255"/>
    <field name="wechatId" type="string" column="wechat_id" length="255"/>
    <field name="unionId" type="string" column="union_id" length="255"/>
    <field name="reFrom" type="integer" column="re_from"/>
    <field name="status" type="integer" column="status"/>
    <field name="createTime" type="integer" column="create_time"/>
    <field name="modifyTime" type="integer" column="modify_time"/>
  </entity>
</doctrine-mapping>

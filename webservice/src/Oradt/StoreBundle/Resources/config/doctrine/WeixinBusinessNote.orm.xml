<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\WeixinBusinessNote" table="weixin_business_note">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="cardId" type="string" column="card_id" length="255"/>
    <field name="noteType" type="integer" column="note_type"/>
    <field name="note" type="string" column="note" length="5000"/>
    <field name="picture" type="string" column="picture" length="500"/>
    <field name="address" type="string" column="address" length="255"/>
    <field name="createTime" type="integer" column="create_time"/>
    <field name="beginTime" type="integer" column="begin_time"/>
    <field name="endTime" type="integer" column="end_time"/>
    <field name="modifyTime" type="integer" column="modify_time"/>
    <field name="addId" type="integer" column="add_id"/> 
    <field name="bizId" type="string" length="100" column="biz_id"/>
    <field name="isDel" type="integer" column="is_del"/> 
  </entity>
</doctrine-mapping>

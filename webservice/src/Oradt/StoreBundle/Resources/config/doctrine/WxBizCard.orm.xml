<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oradt\StoreBundle\Entity\WxBizCard" table="wx_biz_card">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bizId" type="string" column="biz_id" length="40" nullable="false"/>
    <field name="cardId" type="string" column="card_id" length="32" nullable="false"/>
    <field name="userId" type="string" column="user_id" length="20" nullable="false"/>
    <field name="vcard" type="text" column="vcard"  nullable="false"/>
    <field name="markPoint" type="text" column="mark_point" nullable="false"/>
    <field name="status" type="string" column="status" nullable="false"/>
    <field name="picture" type="string" column="picture" length="256" nullable="false"/>
    <field name="picPathA" type="string" column="pic_path_a" length="256" nullable="false"/>
    <field name="picPathB" type="string" column="pic_path_b" length="256" nullable="false"/>
    <field name="remark" type="string" column="remark" length="256" nullable="false"/>
    <field name="createTime" type="integer" column="create_time" length="10" nullable="false"/>
    <field name="modifiedTime" type="integer" column="modified_time" length="10" nullable="false"/>
    <field name="cardFrom" type="string" column="card_from" length="50" nullable="false"/>
    <field name="cardType" type="string" column="card_type" length="50" nullable="false"/>
    <field name="vcardtxt" type="string" column="vcardtxt" length="15000" nullable="false"/>
  </entity>
</doctrine-mapping>
